<h1>Sign in</h1>
  <%= form_tag(sessions_path) do %>
<div class="form_group">
    <%= label_tag :username %>
  <%= text_field_tag :username %>
  </div>

  <div class="form_group">
      <%= label_tag :password %>
      <%= password_field_tag :password %>
  </div>

    <div class="form_group">
      <%= submit_tag 'Sign in'%>
    </div>

<% end %>
