<h1>Hello</h1>


<h3> Sign in </h3>
<%= form_with scope: :user, url: users_path, local: true do |f| %>

  <p>
    <%= f.label :username %><br>
    <%= f.text_field :username%>
  </p>

  <p>
    <%= f.label :password %><br>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
